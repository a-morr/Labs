@article{Hall.2,
  title={Modeling weight-loss maintenance to help prevent body weight regain},
  author={Hall, K.D. and Jordan, P.N.},
  journal={The American journal of clinical nutrition},
  volume={88},
  number={6},
  pages={1495},
  year={2008},
  publisher={Am Soc Nutrition},
  annote={This paper expands upon Hall's work, describing how much a given lifestyle change in diet and/or physical activity will affect weight.  As a supplement to this paper, Hall and Jordan provide a spreadsheet program that computes their formula.  The computation assumes a person is in steady state when they begin the intervention, and computes the steady state they converge to.  No transient behavior is described in this model.  It is also worth mentioning that this computation requires the computation of the Lambert W function, which solves for the fixed point of an exponential-type function.  There is no closed-form solution to this function, and it must be computed using some sort of optimization routine.}
}

@article{Hall.3,
  title={Predicting metabolic adaptation, body weight change, and energy intake in humans},
  author={Hall, K.D.},
  journal={American Journal of Physiology-Endocrinology And Metabolism},
  volume={298},
  number={3},
  pages={E449},
  year={2010},
  publisher={Am Physiological Soc},
  annote={This paper is the pinnacle of Kevin Hall's dynamic weight change research, and among the most important weight change papers ever written.  In this paper, Hall examines the complex dynamical interactions between lean and fatty tissues, extra-cellular fluid, and consumption.  The model presented contains over 40 constants relating to the chemical and physical processes of digestion, metabolism, physical activity, and physical activity.  A comparison is made with data from over 50 previous studies, where both intake and expenditure were measured and controlled.  Hall's model seems to fit very well.  This is the best and most detailed mathematical model to date for weight change.}
}

@article{Hall.4,
