@article{Ant,
  title={The equations governing weight change in human beings},
  author={Antonetti, V.W.},
  journal={The American Journal of Clinical Nutrition},
  volume={26},
  number={1},
  pages={64},
  year={1973},
  publisher={Am Soc Nutrition},
  annote={This paper is the first to attempt a dynamical model for weight change.  It considers the problem as one of thermodynamics where excess weight is potential energy and energy expenditure, both from basal metabolic processes and physical activity, are storage reducing.  There was no compartmentalization in this model, and thus, no differentiation between fat and lean tissue stores, but it was an early and important step in the ideas, which were used by both Halls group at the NIH, and Westerterp's work before that.  Essentially this paper is an important historical step, worthy of note.}
}

