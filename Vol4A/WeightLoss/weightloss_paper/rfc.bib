@article{Wadden,
  title={Relationship of dieting history to resting metabolic rate, body composition, eating behavior, and subsequent weight loss},
  author={Wadden, T.A. and Bartlett, S. and Letizia, K.A. and Foster, G.D. and Stunkard, A.J. and Conill, A.},
  journal={The American journal of clinical nutrition},
  volume={56},
  number={1},
  pages={203S},
  year={1992},
  publisher={Am Soc Nutrition},
  annote={This paper shows that there is no evidence of a relationship between weight cycling and changes in RMR.  In other words, if someone loses weight, then gains it all back again, it would be expected that their RMR would be on par with what it was before the weight was lost.  It was also demonstrated that people who had done a lot of weight cycling before would not have a harder time losing weight in the future.  In other words, there is no indication that the body would remember losing weight and thus function differently.},
}


@article{W,
  title={Energy intake, physical activity and body weight: a simulation model},
