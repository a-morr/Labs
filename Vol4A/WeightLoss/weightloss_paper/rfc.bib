

@article{Mo,
  title={Changes in diet and lifestyle and long-term weight gain in women and men},
  author={Mozaffarian, D. and Hao, T. and Rimm, E.B. and Willett, W.C. and Hu, F.B.},
  journal={New England Journal of Medicine},
  volume={364},
  number={25},
  pages={2392--2404},
  year={2011},
  publisher={Mass Medical Soc},
  annote={This paper reports the dietary habits of 120,877 people who were studied over several years, as many as 20 for many of the subjects.  People were categorized in different ways, from health, diet and physical activity, and their weight changes were recorded over time.  Making no claims about causation, this paper reports that healthy and normal weighted people had, in general, different habits than those who were heavy, or got heavy over the course of the study, thus suggesting some further areas of research, and possible strategies for better weight management.  For example, a regular diet including fruit, vegetables, yogurt and nuts were more highly correlated (not surprisingly) with healthier people.  This paper is largely confirmatory with conventional wisdom, with the possible exception of the correlation found between normal weighted people and regular consumption of nuts and yogurt, which may be viewed by many as less healthy due to their higher levels of fat.}
}

